    <section id="id1167681856779">
      <title>Memorandum</title>
      <para id="id1167676240600">Opdrag 1</para>
      <para id="id1167678377551">(a)</para>
      <para id="id4331174">(b) flouer</para>
      <para id="id1167676240055"> gehalveer</para>
      <para id="id1167676163576"> Die stroom sal nie vloei nie.</para>
      <para id="id1167678382911">(c) Ja</para>
      <para id="id7302759"> Dieselfde</para>
      <para id="id3176496"> Die ander gloeilampie sal bly brand.</para>
      <para id="id1167676068899">(d) parallel</para>
      <para id="id1167678026302">(e) warm</para>
      <para id="id1426629"> warmte</para>
